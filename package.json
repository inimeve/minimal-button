        "node-sass": "^4.9.4",
        "rimraf": "^2.6.2",
        "typescript": "^2.6.2"
    },
    "peerDependencies": {
        "bootstrap": "^3.3.0"
    },
    "dependencies": {
        "angular": "^1.7.5"
    }
}
