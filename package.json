{
    "name": "minimal-button",
    "description": "Minimal button for angularJs",
    "version": "0.0.12",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rimraf dist && npm run styles && npm run html && tsc",
        "html": "copyfiles src/*.html ./dist -f",
